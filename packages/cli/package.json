{
  "name": "gopx-webui",
  "version": "1.0.6",
  "description": "CLI tool for installing GOPX WEBUI Components.",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "gopx-webui": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist && rimraf components",
    "start:dev": "cross-env COMPONENTS_REGISTRY_URL=http://localhost:3003 node dist/index.js",
    "start": "node dist/index.js",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "release": "changeset version",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm build && pnpm publish --access public",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GopalVerma1303/webui.gopx.git",
    "directory": "packages/cli"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "gopx",
    "webui",
    "components",
    "ui",
    "tailwind",
    "framer-motion",
    "nextjs"
  ],
  "author": {
    "name": "Gopal Verma",
    "url": "https://twitter.com/bettercallgopal"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GopalVerma1303/webui.gopx/issues"
  },
  "homepage": "https://webui.gopx.dev",
  "devDependencies": {
    "@gopx-webui/typescript-config": "workspace:*",
    "@types/babel__core": "^7.20.1",
    "@types/diff": "^5.0.3",
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash.template": "^4.5.1",
    "@types/node": "^20",
    "@types/prompts": "^2.4.2",
    "rimraf": "^4.1.3",
    "tsup": "^8.2.4",
    "type-fest": "^3.8.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.4",
    "@babel/core": "^7.22.1",
    "@babel/parser": "^7.22.6",
    "@babel/plugin-transform-typescript": "^7.22.5",
    "chalk": "5.2.0",
    "commander": "^12.1.0",
    "configstore": "^7.0.0",
    "cosmiconfig": "^8.1.3",
    "diff": "^5.1.0",
    "execa": "^7.0.0",
    "fast-glob": "^3.3.2",
    "figlet": "^1.7.0",
    "fs-extra": "^11.1.0",
    "gradient-string": "^2.0.2",
    "https-proxy-agent": "^6.2.0",
    "lodash.template": "^4.5.0",
    "node-fetch": "^3.3.0",
    "ora": "^6.1.2",
    "prompts": "^2.4.2",
    "recast": "^0.23.2",
    "ts-morph": "^18.0.0",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.20.2"
  }
}
